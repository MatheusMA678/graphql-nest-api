generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id         String  @id @default(uuid()) @map("_id") @db.ObjectId
  name       String
  email      String  @unique
  password   String
  avatar_url String?

  @@map("users")
}
